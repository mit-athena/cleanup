#ifdef vax
CC=gcc
#endif
#ifdef _IBMR2
MACHLIBS=-ls
#endif
#ifdef SOLARIS
MACHLIBS=-lkvm
#endif

SimpleProgram(cleanup, cleanup.o,-lhesiod ${MACHLIBS},/etc/athena/)
#ifdef SOLARIS
install_program(cleanup,-m 4755 -o root,/etc/athena/)
#endif
