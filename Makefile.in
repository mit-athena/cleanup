# $Id: Makefile.in,v 2.3 1999-03-11 22:38:28 danw Exp $

SHELL=/bin/sh
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
VPATH=@srcdir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
lsbindir=@lsbindir@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
DEFS=@DEFS@
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
KRB4_LIBS=@KRB4_LIBS@
AFS_LIBS=-lprot -lauth -lrxkad -lubik @AFS_DIR@/lib/afs/util.a -laudit \
	 -lsys -lrx -llwp -lsys
ALL_CFLAGS=${CPPFLAGS} ${CFLAGS} ${DEFS}

all: cleanup

cleanup: cleanup.o
	${CC} ${LDFLAGS} -o cleanup cleanup.o -lal -llocker -lzephyr \
		-lhesiod ${AFS_LIBS} ${KRB4_LIBS} ${LIBS}

.c.o:
	${CC} -c ${ALL_CFLAGS} $<

check:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${lsbindir}
	${INSTALL_PROGRAM} -m 555 cleanup ${DESTDIR}${lsbindir}

clean:
	rm -f cleanup.o cleanup

distclean: clean
	rm -f config.cache config.log config.status Makefile
